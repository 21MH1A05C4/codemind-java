import java.util.*;
class Square
{
public static void main(String args[])
{
	Scanner sc=new Scanner(System.in);
	    int s,i,c=0;
	    s=sc.nextInt();
		for(i=1;i<=s;i++)
		{
		    if(i*i!=s)
		    {
		        c+=1;
		    }
		}
		if(c==1)
		{
		    System.out.format("False");
        }
        else
		    System.out.format("True");
}
}
